{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "cities/{city}/listings/{id}"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "existingDoc",
      "databaseName": "gewoscout-db",
      "collectionName": "ListingsByCity",
      "id": "{id}",
      "partitionKey": "{city}",
      "connectionStringSetting": "CosmosDBConnectionString"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}